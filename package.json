{
  "name": "flowbite-svelte-starter",
  "version": "0.1.3",
  "description": "Flowbite-Svelte starter kit",
  "author": {
    "name": "Shinichi Okada",
    "email": "connect@codewithshin.com",
    "url": "https://blog.codewithshin.com"
  },
  "bugs": "https://github.com/shinokada/flowbite-svelte-starter/issues",
  "homepage": "https://github.com/shinokada/flowbite-svelte-starter",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@catppuccin/tailwindcss": "^0.1.6",
    "@playwright/test": "^1.45.3",
    "@sveltejs/adapter-auto": "2.0.0",
    "@sveltejs/kit": "^2.5.20",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vite-pwa/sveltekit": "^0.6.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "flowbite": "^1.8.1",
    "flowbite-svelte": "^0.46.15",
    "flowbite-svelte-blocks": "^0.1.6",
    "flowbite-svelte-icons": "^1.6.1",
    "postcss": "^8.4.40",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^3.59.2",
    "svelte-check": "^2.10.3",
    "svelte-heros": "^2.9.2",
    "tailwindcss": "^3.4.7",
    "tslib": "^2.6.3",
    "typescript": "~4.7.4",
    "vite": "^4.5.3",
    "vitest": "^0.28.5"
  },
  "type": "module",
  "keywords": [
    "svelte",
    "sveltekit",
    "flowbite",
    "flowbite-svelte",
    "tailwindcss"
  ],
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shinokada/flowbite-svelte-starter.git"
  },
  "pnpm": {
    "overrides": {
      "@sveltejs/kit@<1.15.1": ">=1.15.1",
      "@sveltejs/kit@<1.15.2": ">=1.15.2",
      "undici@<5.26.2": ">=5.26.2",
      "undici@<5.28.4": ">=5.28.4",
      "undici@<=5.28.2": ">=5.28.3"
    }
  },
  "dependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.1"
  }
}
